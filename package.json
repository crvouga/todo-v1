{
  "name": "todo",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "test:unit": "vitest --environment jsdom --root src/",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -w -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "server:dev": "npx ts-node-dev --project ./tsconfig.server.json src/main.server.ts"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.201.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "daisyui": "^2.38.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "firebase-admin": "^11.2.0",
    "morgan": "^1.10.0",
    "pinia": "^2.0.23",
    "uuid": "^9.0.0",
    "vue": "^3.2.41",
    "vue-router": "^4.1.5",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/jsdom": "^20.0.0",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.11.68",
    "@types/uuid": "^8.3.4",
    "@vitejs/plugin-vue": "^3.1.2",
    "@vitejs/plugin-vue-jsx": "^2.0.1",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^2.1.0",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.22.0",
    "eslint-plugin-vue": "^9.3.0",
    "jsdom": "^20.0.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.18",
    "prettier": "^2.7.1",
    "tailwindcss": "^3.2.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "~4.7.4",
    "vite": "^3.1.8",
    "vitest": "^0.24.3",
    "vue-tsc": "^1.0.8"
  }
}
